{
  "name": "@cfw-boilerplate/api",
  "version": "0.0.1",
  "private": true,
  "source": "src/index.ts",
  "type": "module",
  "scripts": {
    "========= Run": "",
    "deploy": "wrangler deploy",
    "dev": "wrangler dev --live-reload --local --config ./wrangler/wrangler.toml",
    "dev:qa": "wrangler dev --live-reload --env qa --config ./wrangler/wrangler.qa.toml.json",
    "dev:uat": "wrangler dev --live-reload --env uat --config ./wrangler/wrangler.uat.toml.json",
    "dev:prod": "wrangler dev --live-reload --env prod --config ./wrangler/wrangler.prod.toml.json",
    "start": "wrangler dev",
    "========= Build": "",
    "build": "node ../scripts/src/bild/build-worker.mjs --entry ./src/index.ts --out ./build/worker.mjs",
    "========= DB": "",
    "file": "tsx ../scripts/wrangle.ts --cmds db:file:./sql/db_v1.sql",
    "proxy": "pnpm exec cf-bindings-proxy --local --d1=local-CFW_VUE_AI_DB_LOCAL",
    "proxy:exec": "pnpm exec wrangler d1 execute local-CFW_VUE_AI_DB_LOCAL --local --config=../api/wrangler.dev.toml --command",
    "proxy:mig:list": "pnpm exec wrangler d1 migrations list local-CFW_VUE_AI_DB_LOCAL --local --config=../api/wrangler.dev.toml",
    "proxy:mig:apply": "pnpm exec wrangler d1 migrations apply local-CFW_VUE_AI_DB_LOCAL --local --config=../api/wrangler.dev.toml"
  },
  "dependencies": {
    "@cloudflare/itty-router-openapi": "^1.0.5",
    "@lucia-auth/adapter-sqlite": "^2.0.1",
    "@auth/kysely-adapter": "^0.1.5",
    "kysely": "^0.26.3",
    "kysely-d1": "^0.3.0",
    "lucia": "^2.7.4",
    "openai": "^4.20.1",
    "providers": "link:@lucia-auth/oauth/providers",
    "universal-cookie": "^6.1.1",
    "vike": "^0.4.147"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@cloudflare/workers-types": "^4.20231121.0",
    "@types/node": "^20.10.1",
    "@types/service-worker-mock": "^2.0.1",
    "cf-bindings-proxy": "^0.7.1",
    "itty-router": "^4.0.23",
    "typescript": "^5.3.2",
    "wrangler": "^3.19.0",
    "better-sqlite3": "^9.1.1",
    "chalk": "^5.3.0",
    "yargs": "^17.7.2"
  }
}