{
  "name": "cfw-boilerplate",
  "version": "0.0.1",
  "workspaces": [
    "api",
    "scripts",
    "types",
    "utils",
    "ui"
  ],
  "type": "module",
  "scripts": {
    "========= Util": "",
    "wrangle": "pnpm scripts wrangle",
    "start": "cross-env NODE_ENV=development && concurrently \"pnpm serve\" \"pnpm v:dev\"",
    "dev": "pnpm ui build:w && pnpm w:build && pnpm w:dev",
    "qa": "pnpm ui build:w && pnpm w:build && pnpm w:qa",
    "prev": "pnpm run wrangle:kv && pnpm run w:dev",
    "lint": "eslint --ext .ts,.tsx .",
    "prepare": "cd .. && husky install app/.husky",
    "========= Wskp": "",
    "api": "pnpm --filter api",
    "db": "pnpm --filter db",
    "types": "pnpm --filter types",
    "scripts": "pnpm --filter scripts",
    "serv": "pnpm --filter server",
    "ui": "pnpm --filter ui",
    "utils": "pnpm --filter utils",
    "========= Server": "",
    "serve": "pnpm serv serve",
    "========= Api": "",
    "w:build": "pnpm scripts build",
    "w:dev": "pnpm api dev",
    "w:qa": "pnpm api dev:qa",
    "w:deploy": "pnpm api deploy",
    "========= Ui": "",
    "v:build": "pnpm ui build",
    "v:dev": "pnpm ui dev"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@cloudflare/workers-types": "^4.20231121.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@t3-oss/env-core": "^0.7.1",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "git-last-commit": "^1.0.1",
    "husky": "^8.0.3",
    "kleur": "^4.1.5",
    "lint-staged": "^15.2.0",
    "lucia": "^2.7.4",
    "pinia": "^2.1.7",
    "pino": "^8.17.1",
    "pino-pretty": "^10.3.0",
    "prettier": "3.1.0",
    "typescript": "^5.3.2",
    "unocss": "^0.58.0",
    "vike": "^0.4.147",
    "vue": "^3.3.9",
    "zod": "^3.22.4"
  }
}