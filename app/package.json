{
  "name": "cfw-boilerplate",
  "version": "0.0.1",
  "workspaces": [
    "api",
    "scripts",
    "types",
    "utils",
    "ui"
  ],
  "type": "module",
  "scripts": {
    "========= Util": "",
    "wr": "pnpm scripts wrangle",
    "simple": "cross-env NODE_ENV=development && concurrently \"pnpm simp\" \"pnpm v:dev\"",
    "start": "pnpm serve",
    "d": "pnpm ui build:d && pnpm w:d",
    "dev": "pnpm ui build:w && sudo pnpm w:dev",
    "rng": "pnpm ui build:w && pnpm w:rng",
    "qa": "pnpm ui build:w && pnpm w:qa",
    "prev": "pnpm run wrangle:kv && pnpm run w:dev",
    "lint": "eslint --ext .ts,.tsx .",
    "prepare": "cd .. && husky install app/.husky",
    "dep:d": "pnpm ui build:wp && pnpm w:dep:d",
    "dep:r": "pnpm ui build:wp && pnpm w:dep:r",
    "generate": "git status --porcelain | grep . && echo 'There are pending changes.' || pnpm openapi --input ./api/openapi.json --output ./ui/src --useOptions --indent 2",
    "========= Wskp": "",
    "api": "pnpm --filter api",
    "db": "pnpm --filter db",
    "types": "pnpm --filter types",
    "scripts": "pnpm --filter scripts",
    "serv": "pnpm --filter server",
    "ui": "pnpm --filter ui",
    "utils": "pnpm --filter utils",
    "========= Server": "",
    "serve": "pnpm serv vike",
    "simp": "pnpm serv simple",
    "========= Api": "",
    "w:build": "pnpm scripts build",
    "w:d": "pnpm api dev",
    "w:dev": "pnpm api dev:dev",
    "w:rng": "pnpm api dev:rng",
    "w:qa": "pnpm api dev:qa",
    "w:dep:d": "pnpm api dep:dev",
    "w:dep:r": "pnpm api dep:rng",
    "========= Ui": "",
    "v:build": "pnpm ui build",
    "v:build:p": "pnpm ui build:wp",
    "v:dev": "pnpm ui dev"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@cloudflare/workers-types": "^4.20231121.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@t3-oss/env-core": "^0.7.1",
    "@tanstack/vue-query": "^5.14.4",
    "@types/better-sqlite3": "^7.6.8",
    "@types/lodash-es": "^4.17.12",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "better-sqlite3": "^9.2.2",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "git-last-commit": "^1.0.1",
    "husky": "^8.0.3",
    "kleur": "^4.1.5",
    "lint-staged": "^15.2.0",
    "lodash-es": "^4.17.21",
    "lucia": "^2.7.4",
    "openapi-typescript-codegen": "^0.25.0",
    "pinia": "^2.1.7",
    "pino": "^8.17.1",
    "pino-pretty": "^10.3.0",
    "prettier": "3.1.0",
    "typescript": "^5.3.2",
    "universal-cookie": "4.0.4",
    "unocss": "^0.58.0",
    "vike": "^0.4.147",
    "vue": "^3.3.9",
    "zod": "^3.22.4"
  }
}